{
   "name": "vendorpm-be-assessment",
   "version": "1.0.0",
   "main": "handler.js",
   "keywords": [],
   "author": "VendorPM",
   "license": "UNLICENCED",
   "private": true,
   "scripts": {
      "build": "tsc --build",
      "clean": "tsc --build --clean",
      "lint": "prettier -c handler.ts test/ src/ && eslint handler.ts test/ src/",
      "lint:fix": "prettier --write handler.ts test/ src/ && eslint handler.ts test/ src/ --fix",
      "migrate:generate": "yarn typeorm migration:generate -d src/data-source.ts -p",
      "migrate:new": "yarn typeorm migration:create",
      "migrate:revert": "yarn typeorm migration:revert -d src/data-source.ts",
      "migrate:run": "yarn typeorm migration:run -d src/data-source.ts",
      "migrate:show": "yarn typeorm migration:show -d src/data-source.ts",
      "prepare": "husky install",
      "start": "serverless offline -t --reloadHandler --httpPort 3001",
      "test": "jest test",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:query": "yarn typeorm query -d src/data-source.ts",
      "seed": "ts-node src/seeds/index.ts"
   },
   "devDependencies": {
      "@types/aws-lambda": "^8.10.101",
      "@types/better-sqlite3": "^7.6.0",
      "@types/jest": "^28.1.6",
      "@types/node": "^16.11.10",
      "@types/uuid": "^9.0.8",
      "@types/winston": "^2.4.4",
      "@typescript-eslint/eslint-plugin": "^5.30.6",
      "@typescript-eslint/parser": "^5.30.6",
      "better-sqlite3": "^7.6.2",
      "eslint": "8.22.0",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-import-resolver-typescript": "^3.5.1",
      "eslint-plugin-import": "^2.26.0",
      "husky": "^8.0.1",
      "jest": "^28.1.3",
      "prettier": "^2.7.1",
      "serverless": "^3.22.0",
      "serverless-dotenv-plugin": "^4.0.2",
      "serverless-offline": "^11.5.0",
      "serverless-plugin-typescript": "^2.1.2",
      "ts-jest": "^28.0.7",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "dotenv": "^16.4.5",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "sqlite3": "^5.1.4",
      "typeorm": "0.3.12",
      "uuid": "^9.0.1",
      "winston": "^3.12.0",
      "zod": "^3.21.4"
   }
}
